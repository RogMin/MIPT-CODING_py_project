   <string notr="true">* {
	border: none;
	background-color: transparent;
	background: none;
	color: #fff;
	font-family: Montserrat SemiBold;
}

#centralwidget {
	background-color: #1f232a;
}

QScrollArea {
	background-color: #1f232a;
}

QLineEdit {
	background-color: #272b33;
	border: 2px solid #272b33;
 	border-radius: 5px;
}

QPushButton {
	background-color: #272b33;
	border: 2px solid #272b33;
 	border-radius: 5px;
}

QPushButton::pressed {
	background-color: #1f232a;
}

QComboBox {
	background-color: #272b33;
	border: 2px solid #272b33;
 	border-radius: 5px;
	padding-left: 2px;
}

QComboBox::down-arrow{
    image: url(:/icons/icons/chevron-down.svg);
}

QComboBox::drop-down{
    border: none;
}

QComboBox QAbstractItemView
{
    color: black;
    selection-background-color: #1f232a;
}

QSlider::groove:horizontal { 
	background-color: #272b33;
	border: 0px solid #272b33; 
	height: 8px; 
	border-radius: 4px;
}

QSlider::handle:horizontal { 
	background-color: rgb(64, 71, 85);
	border: 4px solid rgb(64, 71, 85); 
	width: 10px; 
	height: 10px; 
	line-height: 10px; 
	margin-top: -5px; 
	margin-bottom: -5px; 
	border-radius: 9.45px; 
}

QSlider::handle:horizontal:hover { 
	border-radius: 9.45px;
}

QScrollBar {              
	border: none;
	width: 10px;
	background: #272b33;
}
QScrollBar::handle {
	border-radius: 4px;
	background: #383e4a;
}
QScrollBar::sub-page {
    background: #272b33;
}

QScrollBar::add-page {
    background: #272b33;
}
QScrollBar::add-line {       
	height: 0px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QScrollBar::sub-line {
	height: 0 px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
</string>
